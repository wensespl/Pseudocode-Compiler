ENTERO SUBRUTINA busquedaLineal(ENTERO objetivo)
    ENTERO arr[4]
    arr[0] = 1
    arr[1] = 2
    arr[2] = 3
    arr[3] = 4

    PARA i = 0 HASTA 4
      SI arr[i] == objetivo ENTONCES
        DEVOLVER i
      FINSI
    FINPARA

    DEVOLVER -1
FINSUBRUTINA

resultado = busquedaLineal(3)

